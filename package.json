{
  "name": "vem",
  "version": "0.0.3",
  "description": "Vuejs 2.0 Easy Maps with OpenLayers",
  "main": "./dist/main.js",
  "scripts": {
    "build": "poi build -m --format umd --module-name vem ./src/index.js && npm run build-docs",
    "build-docs": "vuepress build ./src/docs",
    "test": "poi test ./src/index.js",
    "dev": "poi ./example/index.js",
    "watch": "poi watch ./src/index.js",
    "docs": "vuepress dev ./src/docs"
  },
  "keywords": [
    "vue",
    "vue2",
    "vuejs",
    "vuejs2",
    "component",
    "webcomponent",
    "maps",
    "openlayers",
    "openlayers3",
    "openlayers4",
    "spatial"
  ],
  "author": "koehr <n@koehr.in>",
  "license": "MIT",
  "dependencies": {
    "ol": "^5.1"
  },
  "devDependencies": {
    "node-sass": "^4.9.0",
    "poi": "^10.2",
    "propdoc": "^0.9.3",
    "raw-loader": "^0.5",
    "sass-loader": "^7.0.3",
    "vuepress": "^0.12.0"
  }
}
